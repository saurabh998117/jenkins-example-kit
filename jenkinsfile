pipeline {
    agent any
    stages {
        stage("Compile") {
            steps {
                echo "Compiling Java file..."
                sh 'javac Test.java'
                sh 'ls -l' // Optional: Shows if Test.class is generated
            }
        }
        stage("Run") {
            steps {
                echo "Running Java program..."
                sh 'java -cp . Test'
            }
        }
    }
}
